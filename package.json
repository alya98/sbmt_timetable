{
  "name": "sbmt_timetable",
  "displayName": "Расписание ИБ",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "eslint ./src/ --fix",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "server": "node ./server/server.js",
    "ios": "react-native run-ios",
    "iphone7": "react-native run-ios --simulator=\"iPhone 7\"",
    "android": "react-native run-android",
    "postinstall": "node ./patchers/gesturePatcher && node ./patchers/runIOSPatcher"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-dropdownalert": "^3.7.0",
    "react-native-gesture-handler": "1.1.0",
    "react-native-picker-select": "^5.1.0",
    "react-native-router-flux": "4.0.6",
    "react-native-swipe-gestures": "1.0.2",
    "react-native-swipeout": "^2.3.6",
    "react-native-xml2js": "^1.0.3",
    "react-redux": "^5.0.7",
    "reactotron-react-native": "^2.1.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/generator": "^7.0.0",
    "@babel/runtime": "^7.9.2",
    "babel-core": "^6.0.0",
    "babel-jest": "23.6.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "metro-react-native-babel-preset": "0.48.1",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native",
    "cacheDirectory": "./cache",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ]
  }
}
